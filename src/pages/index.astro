---
const response = await fetch('https://stackable.club/version-test/api/1.1/obj/course');
const data = await response.json();

// Assuming your Bubble data comes in this format:
// { "response": { "results": [ { "field1": "...", "field2": "..." }, ... ] } }

const items = data.response.results;
---

<html>
  <head>
    <title>My Astro Page</title>
  </head>
  <body>
    <h1>Bubble Data</h1>
    <ul>
      {items.map((item) => (
        <li>
          <strong>{item.Name}</strong>: {item.Description}
        </li>
      ))}
    </ul>
  </body>
</html>
